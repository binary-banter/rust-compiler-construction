struct Triple {
    a: Int,
    b: Int,
    c: Int,
}

fn is_triple(triple: Triple) -> Bool {
    triple.a * triple.a + triple.b * triple.b == triple.c * triple.c
}

fn main() {
    let mut a = 0;
    while (a = read(); a != 0) {
        let triple = Triple { a: a, b: read(), c: read() };

        if is_triple(triple) {
            print(1);
        } else {
            print(0);
        }
    }
}
